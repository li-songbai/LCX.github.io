lt;span class=&quot;line&quot;&gt;22&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;23&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;td class=&quot;code&quot;&gt;&lt;pre&gt;&lt;span class=&quot;line&quot;&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;attr&quot;&gt;&amp;lt;Huawei&amp;gt;system-view&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;attr&quot;&gt;sysname&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;S1&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1]vlan&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;batch 2 3&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1]interface&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;GigabitEthernet 0/0/1&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1-GigabitEthernet0/0/1]port&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;link-type access&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1-GigabitEthernet0/0/1]port&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;default vlan 2     #将该端口加入到VLAN 2&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1]interface&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;GigabitEthernet 0/0/2&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1-GigabitEthernet0/0/2]port&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;link-type access&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1-GigabitEthernet0/0/2]port&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;default vlan 3&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1]interface&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;GigabitEthernet 0/0/3&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1-GigabitEthernet0/0/3]port&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;link-type trunk&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1-GigabitEthernet0/0/3]port&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;trunk allow-pass vlan 2 3&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;comment&quot;&gt;#只在S1中进行配置&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1]interface&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;vlanif 2&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1-Vlanif10]ip&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;address 192.168.2.254 24&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1]interface&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;vlanif 3&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;meta&quot;&gt;[S1-Vlanif20]ip&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;address 192.168.3.254 24&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
    
